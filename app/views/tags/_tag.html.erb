<span class='tag'/>
<% if  current_user.admin? %>
  <%= link_to "x", remove_ticket_tag_path(@ticket, tag),:method => :delete,:remote => true,:id => "delete-#{tag.name.parameterize}" %> 
  <%= tag.name %>
  <%= link_to tag.name,
      search_project_tickets_path(@ticket.project,
      :search => "tag:#{tag.name}") %>
  <% end %>
</span>
